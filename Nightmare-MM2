						thrust.Location = targetHRP.Position
					end)
				end
	
				for _, player in pairs(Players:GetPlayers()) do
					if player ~= lp and hasKnife(player) then
						coroutine.wrap(flingPlayer)(player)
					end
				end
	
			end
		end
	})
